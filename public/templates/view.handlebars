<div id="bundle-container">

        <div class="col-md-6">

            <h3>Bundle : {{bundleId}}</h3>

            <ul class="list-group">
                {{#each bundleUrls}}
                    <li class="list-group-item">

                        <a href="{{url}}" target="_blank">{{#if title}}{{title}}{{else}}{{url}}{{/if}}</a>

                        <div><strong>Details :</strong> {{text}}</div>

                    </li>
                {{else}}
                    <p>No urls present</p>
                {{/each}}
            </ul>

        </div>

        <div class="col-md-4">

        </div>

</div>



